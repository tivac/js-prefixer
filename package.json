{
  "name": "js-prefixer",
  "version": "1.1.0",
  "description": "Prefix urls in javascript code with a cdn URL",
  "main": "./lib/prefixer.js",
  "scripts": {
    "cover": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "lint": "eslint .",
    "mocha": "mocha",
    "test": "npm run lint && mocha",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/node-js-prefixer.git"
  },
  "author": "Pat Cavit <pcavit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tivac/node-js-prefixer/issues"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-arenanet": "^3.0.1",
    "istanbul": "^0.4.2",
    "mocha": "^3.0.0"
  },
  "dependencies": {
    "rocambole": "^0.7.0"
  },
  "engines": {
    "node": ">=0.10"
  },
  "directories": {
    "test": "tests",
    "lib": "lib"
